{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["dist"],
    "rules": {
        "consistent-return": 0,
        "array-callback-return": 0,
        "no-prototype-builtins": 0,
        "no-restricted-syntax": 0, 
        "no-console": 0
    }
}
